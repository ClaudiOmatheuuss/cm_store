<!DOCTYPE html>
<html lang="en">
<%- include('partials/head.ejs') %>

  <body>
    <%- include('partials/navbar.ejs') %>
      <div class="content">
        <% if (products.length> 0) { %> <% products.forEach(product=> { %>

            <h3 class="title">
              <%= product.name %>
            </h3>

            <p>
              <%= product.price %>
            </p>
            <hr>
            <% }) %>
              <% } else { %>
                <p>Não há usuarios para exibir...</p>
                <% } %>
      </div>
  </body>

</html>